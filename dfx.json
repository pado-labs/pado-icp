{
  "canisters": {
    "padoportal": {
      "main": "canisters/padoportal/main.mo",
      "type": "motoko"
    },
    "assets": {
      "dependencies": [
        "padoportal"
      ],
      "frontend": {
        "entrypoint": "build/index.html"
      },
      "source": [
        "build/"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "ic": {
      "providers": [
        "https://mainnet.dfinity.network"
      ],
      "type": "persistent"
    }
  },
  "version": 1,
  "dfx": "0.15.0"
}
